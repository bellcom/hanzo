{% extends layout %}
{% block content %}

{% if page_type|default('page') != 'frontpage' %}
  <h1>{{ page.title }}</h1>
{% endif %}
  <article>
    {{ parse(page.content) }}
  </article>
{% endblock %}

{% block javascript %}
{% if page_type|default('page') == 'frontpage' %}
<script src="{{ cdn|replace({'http:':''}) }}fx/scripts/frontpage.js"></script>
{% endif %}
{% endblock %}
