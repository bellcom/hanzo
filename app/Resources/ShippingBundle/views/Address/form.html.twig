<form action="{{ path('_shipping_address_save', {type: type}) }}" method="post" {{ form_enctype(form) }} class="address {{ type }}" data-addresstype="{{ type }}" data-callback="validateAddress">
  <h3>{{ (type~'.address')|replace('_', '.')|trans({}, 'checkout') }}</h3>
  {{ form_widget(form) }}
</form>
