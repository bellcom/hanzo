<link href='//fonts.googleapis.com/css?family=Merriweather:400,700,900' rel='stylesheet' type='text/css'>
 {% stylesheets
  'fx/css/boilerplate.css'
  'fx/css/faq.css'
  'fx/scripts/colorbox/example4/colorbox.css'
  'fx/scripts/video-js/video-js.css'
