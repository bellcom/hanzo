{% endstylesheets %}

<script type="text/javascript" src="/fx/scripts/modernizr-2.0.6.min.js"></script>
<script type="text/javascript">
var base_url = '{{ url('_homepage') }}';
</script>

</head>
<body id="body-{{ page_type|default('page') }}" class="{{ body_classes|default('cms global') }}">

<div id="container">
  <a href="{{ path('_homepage') }}" title="{{ "Home"|trans }}">
    {{ fx_image_tag('images/POMPdeLUX_logo.png', '', {'id':'logo', 'alt':'POMPdeLUX'}) }}
  </a>

    <div id="main" role="main">
{# <a href="{{ path('basket_add', { 'master' : 'Silas PULLOVER', 'size' : 104, 'color' : 'Navy', 'quantity' : 10 }) }}">{{ "Add to basket"|trans }}</a> #}
      {% render 'HanzoCMSBundle:Menu:menu' with {'type': 'sub', 'from': 400, 'offset': 482 } %}
      {% block content %}
      {% endblock %}
    </div><!-- /div#main -->

  <nav class="main">
    {% render 'HanzoCMSBundle:Menu:menu' with {'type': 'main', 'from': 20 } %}
  </nav><!-- /nav.main -->

  <aside id="secondary-links" role="secondary-links">
    <ul>
      <li><a href="/account">{{ "My account"|trans }}</a></li>
      <li id="select-domain">
        <a class="open-menu" href="/select_domain">Change Country</a>
        <div style="display: none">
          <a href="http://www.pompdelux.dk/">Denmark</a><br><br>
          <a href="http://www.pompdelux.com/">International</a><br><br>
          <a href="http://www.pompdelux.nl/">Netherlands</a><br><br>
          <a href="http://www.pompdelux.no/">Norway</a><br><br>
          <a href="http://www.pompdelux.se/">Sweden</a><br>
        </div>
      </li>
      <li id="mini-basket" class="basket">
        <a href="{{ path('basket_show') }}" title="{{ "Show basket"|trans }}"> {% render 'BasketBundle:Default:miniBasket' %} </a>
      </li>
      <li><a href="{{ path('basket_show') }}">{{ "Show basket"|trans }}</a></li>
    </ul>
  </aside>

{% if page_type == 'frontpage' %}
  <aside id="teasers" role="teasers">
    <ul>
      <li class="teaser-box-1">
        <a href="/forside/home-shopping/find-konsulent">
        <h2>Home shopping</h2>
        <p>Book et arrangement og få hele vores kollektion af skønt børnetøj hjem i din egen stue.</p>
        <span class="button">Find konsulent</span>
        </a>
      </li>
      <li class="teaser-box-2">
        <a href="/forside/home-shopping/aabent-hus">
        <h2>Åbent hus</h2>
        <p>Find et Åbent hus arrangement i nærheden af dig.</p>
        <span class="button">Start her</span>
        </a>
      </li>
      <li class="teaser-box-3">
        <h2>Tilmeld nyhedsbrev</h2>
        <br>
        <form action="/newsletter/subscribe" method="post" class="newsletter-subscription-form">
          <input type="hidden" name="list_ids[]" value="1">
          <div class="clearfix">
            <label>Navn:</label>
            <input type="text" name="name" value="" title="Navn">
          </div>
          <div class="clearfix">
            <label>E-mailadresse:</label>
            <input type="email" name="email_address" value="" title="E-mailadresse">
          </div>
          <div class="clearfix">
            <input type="button" class="button" name="commit" value="Tilmeld">
          </div>
        </form>
      </li>
    </ul>
  </aside>
{% endif %}

</div>

<footer>
  <ul>
    <li>{{ fx_image_tag('images/creditcards_dk.png', '', {'alt':'credit cards'}) }}</li>
    <li>POMPdeLUX ApS</li>
    <li>Møgelhøj 8</li>
    <li>DK-8520 Lystrup</li>
    <li>CVR-nr 29622035</li>
    <li><a href="mailto:mail@pompdelux.dk">mail@pompdelux.dk</a></li>
    <li>+45 8678 7871</li>
  </ul>
</footer>

