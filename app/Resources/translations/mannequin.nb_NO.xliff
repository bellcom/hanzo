            <trans-unit id="title">
                <source>title</source>
                <target>Mix Match: %title%</target>
            </trans-unit>
            <trans-unit id="product">
                <source>product</source>
                <target>Produkt</target>
            </trans-unit>
            <trans-unit id="price">
                <source>price</source>
                <target>Pris</target>
            </trans-unit>
            <trans-unit id="color">
                <source>color</source>
                <target>Farge</target>
            </trans-unit>
            <trans-unit id="size">
                <source>size</source>
