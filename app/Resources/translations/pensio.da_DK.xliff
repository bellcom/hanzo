<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file source-language="en" datatype="plaintext" original="file.ext">
    <body>
      <trans-unit id="payment.title">
        <source>payment.title</source>
        <target>Pensio</target>
      </trans-unit>
      <trans-unit id="ideal.desc">
        <source>ideal.desc</source>
        <target>iDeal</target>
      </trans-unit>
      <trans-unit id="failed.heading">
        <source>failed.heading</source>
        <target>Payment failed</target>
      </trans-unit>
      <trans-unit id="failed.description">
        <source>failed.description</source>
        <target>Payment via Pensio Failed for some reason: %message%</target>
      </trans-unit>
      <trans-unit id="failed.go.back.description">
        <source>failed.go.back.description</source>
        <target><![CDATA[
          Go <a href="%back_url%">back</a> the checkout page and choose another payment method.
        ]]></target>
      </trans-unit>
      <trans-unit id="form.order_id">
        <source>form.order_id</source>
        <target>Order ID: %order_id%</target>
      </trans-unit>
      <trans-unit id="form.payment_id">
        <source>form.payment_id</source>
        <target>Payment ID: %payment_id%</target>
      </trans-unit>
      <trans-unit id="form.amount">
        <source>form.amount</source>
        <target>Amount: %amount%</target>
      </trans-unit>
      <trans-unit id="form.heading">
        <source>form.heading</source>
        <target>Payment</target>
      </trans-unit>
      <trans-unit id="form.description">
        <source>form.description</source>
        <target>Some payment description</target>
      </trans-unit>
      <trans-unit id="wait.heading">
        <source>wait.heading</source>
        <target>Please hold on</target>
      </trans-unit>
      <trans-unit id="wait.description">
        <source>wait.description</source>
        <target><![CDATA[
          We are about to redirect you back to %server%, if for some reason this does not happen click <a href="%url%">here</a>.
        ]]></target>
      </trans-unit>
      <trans-unit id="cancel.payment">
        <source>cancel.payment</source>
        <target>Afbryd betalingen</target>
      </trans-unit>
    </body>
  </file>
</xliff>
