  'fx/scripts/dibs-process.js'
  'fx/scripts/wall.js'
  'fx/scripts/events.js'
  'fx/scripts/quickorder.js'

  'fx/scripts/newsletter.js'

  'fx/scripts/gothia.js'
  'fx/scripts/maps.js'
  'fx/scripts/search.js'
  'fx/scripts/mannequin.js'
  'fx/scripts/faq.js'

  'fx/scripts/oldie.js'
  filter='?yui_js'
  output='fx/cache/all.js'
  combine=true
%}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}

{% block javascript %}
{% endblock %}

{% render 'NewsletterBundle:Default:js' %}

<script>if (jaiks) {jaiks.exec();}</script>
{{ google_analytics_tag() }}
</body>
</html>
