<!doctype html>
<!--[if lt IE 7]> <html class="no-js ie6 oldie" lang="{{ html_lang }}" xmlns:og="http://ogp.me/ns#"> <![endif]-->
<!--[if IE 7]>    <html class="no-js ie7 oldie" lang="{{ html_lang }}" xmlns:og="http://ogp.me/ns#"> <![endif]-->
<!--[if IE 8]>    <html class="no-js ie8 oldie" lang="{{ html_lang }}" xmlns:og="http://ogp.me/ns#"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="{{ html_lang }}" xmlns:og="http://ogp.me/ns#"> <!--<![endif]-->
<head>
<title>{{ 'browser.title'|trans }} {{ browser_title|default('') }}</title>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="robots" content="all">

<meta property="og:url" content="{{ app.request.uri }}">
{{ meta_tags() }}

<link rel="dns-prefetch" href="{{ cdn|replace({'http:':''}) }}">
<link rel="dns-prefetch" href="//cdn.jquerytools.org">
<link rel="dns-prefetch" href="//html5shim.googlecode.com">
<link rel="dns-prefetch" href="//fonts.googleapis.com">

<link href="//fonts.googleapis.com/css?family=Merriweather:400,700,900" rel="stylesheet" type="text/css">
<link rel="shortcut icon" href="/fx/images/favicon.ico" title="POMPdeLUX" type="image/ico">

{% stylesheets
  'fx/2013s1/css/960_8_3_3.css'
  'fx/2013s1/css/jquery.fullimagebox.css'

  'fx/css/colorbox/theme4/colorbox.css'
  'fx/css/dialoug.css'
  'fx/css/jquery-ui.css'
  'fx/scripts/video-js/video-js.css'
  'fx/scripts/fullcalendar/fullcalendar.css'

  'fx/2013s1/css/screen.css'
  'fx/2013s1/css/style.css'

  output='fx/cache/all_2013s1.css'
  combine=true
%}
<link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}

<script type="text/javascript" src="{{ cdn }}fx/scripts/modernizr-2.5.3.js"></script>
</head>
<body id="body-{{ page_type|default('page') }}" class="{{ body_classes|default('') }} {{ html_lang }}">

<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/{{ locale }}/all.js#xfbml=1";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>

<div id="container" class="container container_8">
  <header class="grid_8 row">
    <a href="{{ path('_homepage', {'_locale' : app.session.locale}) }}" title="{{ "home"|trans }}">
      {{ fx_image_tag('images/POMPdeLUX_logo_SS12.png', '0x100', {'id':'logo', 'alt':'POMPdeLUX'}) }}
    </a>
    <nav class="menu main-menu horizontal">
      {% render 'CMSBundle:Menu:menu' with {'type': left_menu.type, 'thread': left_menu.thread, 'from': left_menu.from } %}
    </nav><!-- /nav.main -->
    <hr class="right">
  </header>

  <div id="main" role="main" class="main clearfix grid_8">
    {% block content %}{% endblock %}
  </div><!-- /div#main -->

  <nav id="secondary-links">
    <ul class="horizontal menu right">
      <li><a href="{{ path('_account', {'_locale' : app.session.locale}) }}">{{ "my.account"|trans({}, 'account') }}</a></li>
      <li id="select-domain">
        <a class="open-menu" href="#">Change Country</a>
        <div class="dropdown" style="display: none">
          <a href="{{ baseurl }}/da_DK/">Denmark</a><br><br>
        <a href="{{ baseurl }}/fi_FI/">Finland</a><br><br>
          <a href="{{ baseurl }}/en_GB/">International</a><br><br>
          <a href="{{ baseurl }}/nl_NL/">Netherlands</a><br><br>
        <a href="{{ baseurl }}/nb_NO/">Norway</a><br><br>
          <a href="{{ baseurl }}/sv_SE/">Sweden</a><br>
        </div>
      </li>
      <li class="last">
        <form action="#" class="search-form">
          <input type="submit" class="text-button button" value="{{ "button.label"|trans({}, 'search') }}">
          <input type="text" name="search">
        </form>
      </li>
    </ul>
    <ul class="horizontal menu right basket">
      <li id="mini-basket">
        <span class="sprite"></span>
        <a href="{{ path('basket_view', {'_locale' : app.session.locale}) }}" title="{{ "show.basket"|trans }}"> {% render 'BasketBundle:Default:miniBasket' %} </a>
      </li>
      <li class="last"><a href="{{ path('basket_view', {'_locale' : app.session.locale}) }}">{{ "show.basket"|trans }}</a></li>
    </ul>
  </nav>
</div>

<footer class="container container_8">
  <div class="grid_8">
    <hr class="right">
  </div>
  <div class="grid_1-5 footer-menu footer-menu-1">
    {{ "footer.menu_1"|trans|raw }}
  </div>
  <div class="grid_1-5 footer-menu footer-menu-2">
    {{ "footer.menu_2"|trans|raw }}
  </div>
  <div class="grid_1-5 footer-menu footer-menu-3">
    {{ "footer.menu_3"|trans|raw }}
  </div>
  <div class="grid_2-5 footer-menu footer-menu-4">
    <div class="sprite"></div>
    {% render 'NewsletterBundle:Default:block' %}
  </div>
  <div class="grid_8">
    <hr class="left">
  </div>
  <div class="copy grid_8">
    {{ 'footer.address'|trans({'%cdn%':cdn})|raw }}
  </div>
</footer>

<script type="text/javascript">
  var cookie_params = {
    'domain' : '{{ app.request.getHttpHost() }}',
    'path' : '{{ path('_homepage', {'_locale' : app.session.locale}) }}'
  };
  var base_url = '{{ url('_homepage', {'_locale' : app.session.locale}) }}';
  var cdn_url = '{{ cdn }}';
  var counter_offset = 1314691200;
</script>

<script type="text/javascript" src="{{ asset('bundles/bazingaexposetranslation/js/translator.min.js') }}"></script>
<script type="text/javascript" src="{{ url('bazinga_exposetranslation_js', { 'domain_name': 'js', '_locale' : app.session.locale }) }}"></script>

{% javascripts
  'fx/scripts/jquery.min.js'
  'fx/scripts/jquery.tools.min.js'
  'fx/scripts/tinymce/jscripts/tiny_mce/jquery.tinymce.js'
  'fx/scripts/colorbox/colorbox/jquery.colorbox.js'
  'fx/scripts/zoom/jquery.zoom.js'
  'fx/scripts/accounting/accounting.min.js'
  'fx/scripts/jquery.formparams.js'
  'fx/scripts/jquery.cycle2.min.js'
  'fx/2013s1/scripts/jquery.fullimagebox.js'
